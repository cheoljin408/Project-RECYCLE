              <input id='low_price' class="form-control" type="text" placeholder="10,000" /> ~
              <input id='high_price' class="form-control" type="text" placeholder="50,000" />
            </div>
            <div class="row_find">
              <img id='find' style="float:right" src="/images/find.png" />
            </div>

          </div>
        </div>

      </div>
    </div>

